  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/dreamofdark/jupyterlab-mind-map",
  "bugs": {
    "url": "https://github.com/dreamofdark/jupyterlab-mind-map/issues"
